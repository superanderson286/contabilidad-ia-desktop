{
  "$schema": "../node_modules/@tauri-apps/cli/schemas/2.0.0/tauri-app.schema.json",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "productName": "Contabilidad IA App",
  "version": "0.1.0",
  "identifier": "com.tuempresa.contabilidad",
  "plugins": {
    "shell": {
      "open": true
    },
    "window": {
      "enabled": true
    }
  },
  "app": {
    "windows": [
      {
        "title": "Contabilidad IA App",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 600,
        "minHeight": 400,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null,
      "freezePrototype": true,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}